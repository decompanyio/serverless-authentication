{
  "extends": "airbnb",
  "plugins": [],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "func-names": "off",
    "comma-dangle": "off",
    "camelcase": "off",
    "max-len": [
      "error",
      100,
      4,
      {
        "ignorePattern": "/.+/",
        "ignoreUrls": true
      }
    ],
    "strict": "off",
    "prefer-rest-params": "off"
  }
}
